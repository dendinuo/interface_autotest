<?xml version="1.0" encoding="UTF-8"?>
<!-- 多线程 -->
<suite name="thread"   parallel="tests"  threa-count="2">
<!-- methods级别：所有用例都可以在不同的线程下去执行 -->
<!-- classs级别：相同的class标签下的用例在同一个线程中执行，不同的class标签可以在不同的线程下去执行 -->
<!-- tests级别：不同的test标签下的用例可以在不同的线程下去执行 -->
	<test name="demo1">
		<classes>
			<class name="com.interface_autotest.autotest.multiThread.MultiThreadOnXml"/>
			
		</classes>		
	</test>
	<test name="demo2">
		<classes>
			<class name="com.interface_autotest.autotest.multiThread.MultiThreadOnXml"/>	
		</classes>		
	</test>
</suite>

